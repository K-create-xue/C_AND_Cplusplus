#include<iostream>
#include<windows.h>

using namespace std;

void test()
{
	int arry[] = { 1,4,3,6,3,6,3,6,4 };
	for (const int m : arry)
		cout << m << " ";
	int arr_num = sizeof(arry)/sizeof(arry[0]);
	for (int i = 0; i < arr_num ; ++i)
	{
		for (int j = i; j < arr_num; ++j)
		{
			if (arry[i] < arry[j])
				swap(arry[i], arry[j]);
		}
	}
	cout << endl;
	for (const int m : arry)
		cout << m << " ";
}

