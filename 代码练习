int main()
{
	test();
	return 0;
}
int test(int* arr,int n,int key)
{
	
	int low = 0, high = n - 1, mid = 0, index = -1;
	while (low <= high)
	{
		mid = (low + high) / 2;
		if (key == arr[mid])
		{
			index = mid;
		}
		else if (key < arr[mid])
		{
			high = mid-1;
		}
		else
			low = mid+1;
	}
	
	return index;
}
int main()
{
	int key = 0;
	cin >> key;
	int arr[] = { 1,2,3,5,6,7,8,33 };
	int n = 8;
	int num=test(arr,8,key);
	if (num == -1)
		cout << "no found" << endl;
	cout << num << endl;
	return 0;
}
